@model IEnumerable<PruebaTecnicaLibrary.Vo.Entrevista>

@{
    ViewBag.Title = "Pruebas";
}

<h2>Pruebas</h2>

<p>
    @Html.ActionLink("Nueva Entrevista", "AddPruebas")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Reclutadore.Reclutador)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.candicatoname.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Fecha_Entrevista)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Hora_Entrevista)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tipo_Entervista)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Tipo_Tecnologia)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Reclutadore.Reclutador)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.candicatoname.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha_Entrevista)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Hora_Entrevista)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tipo_Entervista)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Tipo_Tecnologia)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
